    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "Components/*": ["src/components/*"],
      "Util": ["src/util/"]
    }
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./src/usecase/ColorPicker/"
  ]
}
